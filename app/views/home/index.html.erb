<section id="warehouses">
  <h2>Galpões</h2>
  <% @warehouses.each do |w| %>
    <%= link_to w.name, warehouse_path(w) %>
    <dl>
      <dt>Código: </dt>
      <dd><%= w.code %></dd>
      <dt>Cidade: </dt>
      <dd><%= w.city %></dd>
      <dt>Área: </dt>
      <dd><%= w.area %> m2</dd>
    </dl>
  <% end %>

  <% if @warehouses.empty? %>
    <p>Não existem galpões cadastrados.</p>
  <% end %>
</section>